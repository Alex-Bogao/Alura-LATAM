<h1> Del 1 al 100 los pares</h1>

<script> 
	function saltarLinea() {
		document.write("<br>") ;
		document.write("<br>") ;
		document.write("<br>") ;
	}

	function imprimir(frase){
			document.write(frase);
			saltarLinea();
	}
	var contador = 0;
	var totalEdades = 0;


	    while(contador <= 100){ 
	    	if(contador %2 ){
	    	imprimir(contador+1)}

	    
	 	contador++ ;
	 } 
	 

	 imprimir("fin");








		</script>
